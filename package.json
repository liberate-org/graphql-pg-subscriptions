    "registry": "https://npm.pkg.github.com/liberate-org"
  },
  "keywords": [
    "pubsub",
    "pub/sub for pg",
    "pub/sub for postgreesql",
    "graphql",
    "graphql-subscriptions",
    "graphql-pg-subscriptions"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liberate-org/graphql-pg-subscriptions.git"
  },
  "peerDependencies": {
    "graphql": "latest"
  },
  "dependencies": {
    "graphql-subscriptions": "^2.0.0",
    "iterall": "^1.3.0",
    "pg-ipc": "^1.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "graphql": "^16.8.1",
    "pg": "^8.11.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.4.5"
  }
