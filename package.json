{
  "name": "@liberate-org/graphql-pg-subscriptions",
  "version": "3.2.6",
  "description": "A graphql subscriptions implementation using postgres and apollo's graphql-subscriptions.",
  "homepage": "https://github.com/liberate-org/graphql-pg-subscriptions",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && npm run build:esm && npm run build.cjs",
